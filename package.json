{
  "name": "react-wedding-website",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run start:client-server",
    "start:client-server": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "cd client && npm run build && npm start",
    "start:server": "cd server && npm start"
  },
  "dependencies": {
    "concurrently": "^6.5.1"
  }
}
